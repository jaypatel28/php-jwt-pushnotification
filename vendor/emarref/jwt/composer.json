{
    "name": "emarref/jwt",
    "description": "A JWT implementation",
    "license": "MIT",
    "authors": [
        {
            "name": "Malcolm Fell",
            "email": "emarref@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "ext-openssl": "Enables more token encryption options"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Emarref\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emarref\\Jwt\\": "test/"
        }
    }
}
